<a class="special-button IconNavLinkAEllement" id="@id" href="@Href">
    <i class="fa @Icon IconNavLinkIEllement" aria-hidden="true" />
    <span class="IconNavLinkSpanEllement">@Text</span>
</a>
@code {
    [Parameter]
    public string Href { get; set; }

    [Parameter]
    public string Icon { get; set; }

    [Parameter]
    public string Text { get; set; }

    [Parameter]
    public string id { get; set; }
}
